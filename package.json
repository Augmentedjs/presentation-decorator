{
  "name": "presentation-decorator",
  "version": "2.5.4",
  "description": "The Augmented.js Next - Presentation Decorator Module.",
  "main": "dist/presentation-decorator.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader --progress",
    "build": "webpack --mode production --module-bind js=babel-loader --progress",
    "lint": "eslint ./src/presentation-decorator.js",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "start": "node index.js",
    "docs": "rm -rf jsdoc && jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/presentation-decorator.git"
  },
  "keywords": [
    "augmentedjs",
    "presentation"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^3.2.2",
    "eslint-config-kentcdodds": "^9.0.2",
    "foodoc": "0.0.9",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.12.0",
    "mocha": "^4.1.0",
    "mocha-jsdom": "^1.2.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^2.1.5"
  },
  "dependencies": {
    "next-core-utilities": "^2.6.0",
    "presentation-dom": "^2.8.0",
    "presentation-mediator": "^2.5.7",
    "presentation-models": "^2.6.0",
    "presentation-widget": "^2.5.4"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/presentation-decorator/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
